/* pages/subordinatePage/contactUs/contactUs.wxss */
page{
  height: 100%;
}

.contact-us-main{
  height: 100%;
  padding-top: 1rpx;
  background-color: #e3f4fd;
}

.qr-code-outer{
  width: 60%;
  padding: 50rpx 30rpx;
  margin: 0 auto;
  margin-top: 120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: #f5fcff;
  border: #f5fcff 1rpx solid;
  border-radius: 20rpx;
  box-shadow: 0 0 30rpx rgb(143, 143, 143);
}

.qr-code-img{
  height: 340rpx;
  width: 340rpx;
  margin-bottom: 20rpx;
}

.des-text{
  font-size: 28rpx;
  color: rgb(102, 102, 102);
  margin-bottom: 40rpx;
}